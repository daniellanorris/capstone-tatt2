{
  "name": "tatt2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export DB_NAME=mongo_project_db && npm run mongo && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mongo": "npm run mongo:restart && npm run mongo:check",
    "mongo:restart": "npm run mongo:stop; npm run mongo:start",
    "mongo:start": "docker run --rm -d -p 27017:27017 --name=mongo-sandbox -e MONGO_INITDB_DATABASE=$DB_NAME mongo:5",
    "mongo:stop": "docker container stop mongo-sandbox",
    "mongo:shell": "docker exec -it mongo-sandbox mongosh",
    "mongo:check": "until docker exec mongo-sandbox mongosh --eval \"use $DB_NAME;\"; do echo ...; sleep 1; done"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.2",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "iron-session": "^6.3.1",
    "js-cookie": "^3.0.5",
    "maplibre-gl": "^3.5.1",
    "mongodb": "^6.1.0",
    "mongoose": "^7.6.2",
    "mongosh": "^2.0.2",
    "next": "^13.5.6",
    "radar-sdk-js": "^3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
